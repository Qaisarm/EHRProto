definition
    OBSERVATION[at0000] matches {    -- Laboratory Test Result
        data matches {
            HISTORY[at0001] matches {    -- Event Series
                events cardinality matches {1..*; unordered} matches {
                    EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
                        data matches {
                            ITEM_TREE[at0003] matches {    -- Tree
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[at0004] occurrences matches {0..1} matches {    -- Test Name
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                    ELEMENT[at0005] occurrences matches {0..1} matches {    -- Test Status
                                        value matches {
                                            DV_CODED_TEXT matches {
                                                defining_code matches {
                                                    [local::
                                                    at0006,    -- Registered
                                                    at0007,    -- Partial
                                                    at0008,    -- Preliminary
                                                    at0009,    -- Final
                                                    at0010,    -- Amended
                                                    at0011]    -- Cancelled
                                                }
                                            }
                                        }
                                    }
                                    CLUSTER[at0012] occurrences matches {0..*} matches {    -- Test Result
                                        items cardinality matches {1..*; unordered} matches {
                                            ELEMENT[at0013] occurrences matches {0..1} matches {    -- Result Name
                                                value matches {
                                                    DV_TEXT matches {*}
                                                }
                                            }
                                            ELEMENT[at0014] occurrences matches {0..1} matches {    -- Result Value
                                                value matches {
                                                    DV_QUANTITY matches {*}
                                                    DV_TEXT matches {*}
                                                    DV_CODED_TEXT matches {*}
                                                }
                                            }
                                            ELEMENT[at0015] occurrences matches {0..1} matches {    -- Reference Range
                                                value matches {
                                                    DV_TEXT matches {*}
                                                }
                                            }
                                            ELEMENT[at0016] occurrences matches {0..1} matches {    -- Interpretation
                                                value matches {
                                                    DV_CODED_TEXT matches {
                                                        defining_code matches {
                                                            [local::
                                                            at0017,    -- Normal
                                                            at0018,    -- High
                                                            at0019,    -- Low
                                                            at0020]    -- Abnormal
                                                        }
                                                    }
                                                }
                                            }
                                            ELEMENT[at0021] occurrences matches {0..1} matches {    -- Comment
                                                value matches {
                                                    DV_TEXT matches {*}
                                                }
                                            }
                                        }
                                    }
                                    ELEMENT[at0022] occurrences matches {0..1} matches {    -- Overall Interpretation
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                    ELEMENT[at0023] occurrences matches {0..1} matches {    -- Clinical Information
                                        value matches {
                                            DV_TEXT matches {*}
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[at0024] matches {    -- Tree
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[at0025] occurrences matches {0..1} matches {    -- Test Method
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0026] occurrences matches {0..1} matches {    -- Laboratory
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0027] occurrences matches {0..1} matches {    -- Specimen Type
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                }
            }
        }
    }

